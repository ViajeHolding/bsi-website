        <section class="banner">
          <div class="banner__carousel carousel slide carousel-fade" data-bs-ride="carousel">
            <div class="carousel-inner">
              <div class="carousel-item carousel-item--01 active"></div>
              <div class="carousel-item carousel-item--02"></div>
              <div class="carousel-item carousel-item--03"></div>
            </div>
          </div>
      
          <div class="banner__info">
            <div class="container">
              <div class="row">
                <div class="col-md-8 col-lg-6">
                  <h1 class="banner__info-title"><%- banner.title %></h1>
            
                  <p class="banner__info-excerpt"><%- banner.excerpt %></p>
                </div>
              </div>
            </div>
          </div>
        </section>

        <section class="company-profile page-section container">
          <div class="row company-profile__row">
            <div class="col-12 col-md-4 company-profile__col">
              <img src="/img/bsi-company-profile.png" alt="BSI company profile" class="company-profile__image" />
            </div>

            <div class="col-12 col-md-8 company-profile__col">
              <div class="company-profile__info">
                <h2 class="company-profile__info-title page-section__title"><%= companyProfile.title %></h2>

                <p class="company-profile__info-body"><%= companyProfile.body %></p>
              </div>
            </div>
          </div>
        </section>

        <section class="roadmap page-section container">
          <div class="row">
            <div class="col">
              <h2 class="roadmap__title page-section__title">Roadmap</h2>
            </div>
          </div>

          <div class="row">
            <div class="col">
              <div class="roadmap__image"></div>
            </div>
          </div>
        </section>

        <section class="introduction page-section container">
          <div class="row introduction__row">
            <div class="col-12 col-md-4 introduction__col">
              <img src="/img/bsi-introduction-token.png" alt="BSI token introduction" class="introduction__image" />
            </div>

            <div class="col-12 col-md-8 introduction__col">
              <div class="introduction__info">
                <h2 class="introduction__info-title page-section__title"><%= introduction.title %></h2>

                <p class="introduction__info-body"><%= introduction.body %></p>
              </div>
            </div>
          </div>
        </section>

        <section class="summary page-section container">
          <div class="row">
            <div class="col">
              <h2 class="summary__title page-section__title"><%= summary.title %></h2>
            </div>
          </div>

          <div class="row">
            <div class="col">
              <p class="summary__body"><%= summary.body %></p>
            </div>
          </div>
        </section>

        <section class="page-banners">
          <div class="page-banners__item page-banners__item--service">
            <div class="container">
              <div class="row">
                <div class="col-12 col-md-8 col-lg-7">
                  <h2 class="page-banners__item-title">Service</h2>
    
                  <p class="page-banners__item-excerpt">Lorem ipsum dolor sit amet consectetur adipisicing elit. Quos praesentium accusamus exercitationem velit fugiat nihil, labore suscipit! Impedit, dolore minima atque quod esse quasi, beatae dolorum tempore corrupti, maxime omnis?</p>
    
                  <a href="/<%= lang %>/service" class="page-banners__item-link btn">Learn More</a>
                </div>
              </div>
            </div>
          </div>

          <div class="page-banners__item page-banners__item--about">
            <div class="container">
              <div class="row">
                <div class="col-12 col-md-8 col-lg-7">
                  <h2 class="page-banners__item-title">About</h2>
    
